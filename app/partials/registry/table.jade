table.table.table-striped
  thead
    tr
      th #id
      th Cognome
      th Nome
  tbody
    tr(ng-repeat="actor in actors | filter: {removed: false}", ng-click="setSelected(actor)", ng-show="!actor.removed")
      td
        {{actor._id}}
      td
        {{actor.cognome}}
      td
        {{actor.nome}}
